{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'ESLint disabled during development for cleaner console'",
    "lint:fix": "echo 'ESLint auto-fix disabled during development'",
    "lint:verbose": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:enable": "eslint . --ext .ts,.tsx,.js,.jsx --quiet",
    "clean-warnings": "npm run update-browserslist",
    "update-browserslist": "npx update-browserslist-db@latest",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss}\""
  },
  "dependencies": {
