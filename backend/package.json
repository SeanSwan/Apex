{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API for security management system",
  "main": "src/server.mjs",
  "type": "module",
  "scripts": {
    "start": "node src/server.mjs",
    "dev": "nodemon src/server.mjs",
    "streaming": "node services/streaming/integrationServer.mjs",
    "dev:streaming": "nodemon services/streaming/integrationServer.mjs",
    "build": "tsc",
    "dev:ts": "node --loader ts-node/esm src/server.ts",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ai": "node test-ai-services.mjs"
  },
  "keywords": [
    "express",
    "api",
    "typescript",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "apn": "^2.2.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.4.0",
    "fs-extra": "^11.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "sanitize-html": "^2.17.0",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "stripe": "^17.7.0",
    "twilio": "^5.4.5",
    "winston": "^3.11.0",
    "ws": "^8.18.0",
    "fluent-ffmpeg": "^2.1.3",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.28",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.11",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^1.3.0",
    "jsdom": "^24.0.0"
  }
}
